{
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "@vercel/php"
      },
      {
        "src": ".vercel/build.sh",
        "use": "@vercel/build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }
  